{
  "name": "flashcat-ci",
  "version": "1.0.0",
  "description": "CLI tool for Flashcat CI/CD operations",
  "main": "dist/index.js",
  "bin": {
    "flashcat-ci": "./dist/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf dist/*",
    "format": "yarn eslint --fix",
    "launch": "ts-node --transpile-only src/cli.ts",
    "launch:debug": "node -r ts-node/register/transpile-only --inspect-brk src/cli.ts",
    "lint": "yarn eslint",
    "prepack": "yarn build && rimraf 'dist/**/__tests__'",
    "test": "jest --colors",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest --runInBand",
    "eslint": "eslint --quiet 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "keywords": [
    "sourcemap",
    "cli",
    "upload"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.8.4",
    "chalk": "3.0.0",
    "clipanion": "^3.2.1",
    "form-data": "^4.0.0",
    "glob": "^10.4.5",
    "inquirer": "^8.2.5",
    "js-yaml": "3.13.1",
    "ora": "5.4.1",
    "proxy-agent": "^6.4.0",
    "semver": "^7.5.3",
    "simple-git": "3.16.0",
    "terminal-link": "2.1.1",
    "typanion": "^3.14.0",
    "tiny-async-pool": "^2.1.0",
    "async-retry": "1.3.1",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/tiny-async-pool": "^2.0.3",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@types/async-retry": "1.4.8",
    "@types/inquirer": "8.2.6",
    "@types/deep-extend": "0.4.31",
    "typescript": "5.3.3",
    "deep-extend": "^0.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "^29.7.0",
    "prettier": "2.0.5",
    "rimraf": "^5.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2"
  }
}
